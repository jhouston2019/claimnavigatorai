# Claim dashboard routing
/app/claim/:claim_id    /app/claim.html?id=:claim_id    200

# Timeline routing
/app/timeline/:claim_id    /app/timeline.html?id=:claim_id    200

# Advisory routing - exclude from catch-all
/app/response-center/situational-advisory/*    /app/response-center/situational-advisory/index.html    200

# Fallback for all other routes
/*    /index.html   200